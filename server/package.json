{
  "name": "hackathon-portal",
  "version": "1.2.2",
  "description": "REST API for ACM at UCSD's hackathon portal.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node --env-file=.env build/index.js",
    "dev": "node -r ts-node/register --env-file=.env --watch index.ts",
    "test": "jest --config jest.config.json --runInBand",
    "release": "typeorm-ts-node-commonjs migration:run -d ./MigrationDataSource.ts && node build/index.js",
    "lint": "eslint ./ --ext .ts",
    "lint:fix": "eslint ./ --ext .ts --fix",
    "db:migrate:generate": "typeorm-ts-node-commonjs migration:generate -d ./MigrationDataSource.ts",
    "db:migrate:create": "typeorm-ts-node-commonjs migration:create",
    "db:migrate": "typeorm-ts-node-commonjs migration:run -d ./MigrationDataSource.ts",
    "db:rollback": "typeorm-ts-node-commonjs migration:revert -d ./MigrationDataSource.ts"
  },
  "repository": "https://github.com/acmucsd/hackathon-portal.git",
  "author": "Eric Chang <55172722+echang594@users.noreply.github.com>",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.974.0",
    "@aws-sdk/lib-storage": "^3.974.0",
    "body-parser": "^2.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "firebase": "^12.8.0",
    "firebase-admin": "^13.6.0",
    "multer": "^2.0.2",
    "pg": "^8.17.2",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.3",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
