{
  "name": "hackathon-portal",
  "version": "1.0.0",
  "description": "REST API for ACM at UCSD's hackathon portal.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js --env-file=.env",
    "dev": "node -r ts-node/register --env-file=.env --watch index.ts",
    "test": "jest --config jest.config.json --runInBand",
    "release": " && node build/index.js --env-file=.env",
    "lint": "eslint ./ --ext .ts",
    "lint:fix": "eslint ./ --ext .ts --fix",
    "db:migrate:generate": "typeorm-ts-node-commonjs migration:generate -d ./MigrationDataSource.ts",
    "db:migrate:create": "typeorm-ts-node-commonjs migration:create",
    "db:migrate": "typeorm-ts-node-commonjs migration:run -d ./MigrationDataSource.ts",
    "db:rollback": "typeorm-ts-node-commonjs migration:revert -d ./MigrationDataSource.ts"
  },
  "repository": "https://github.com/acmucsd/hackathon-portal.git",
  "author": "Eric Chang <55172722+echang594@users.noreply.github.com>",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.723.0",
    "@aws-sdk/lib-storage": "^3.723.0",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "firebase": "^11.0.1",
    "firebase-admin": "^13.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.9",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
